        // create the layout - with data-table wrapper as the layout-content element
        myLayout = $('body').layout({
        	west__size:         200
        ,	south__size:        250
        ,   west__onresize:     function (pane, $pane, state, options) {
                                    var $content    = $pane.children('.ui-layout-content')
